                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </TextField>
                  <Separator opacity="0.0" orientation="VERTICAL" prefHeight="279.0" prefWidth="152.0" VBox.vgrow="ALWAYS" />
                  <HBox>
                     <children>
                        <Button maxWidth="10000.0" mnemonicParsing="false" onAction="#deleteTeam" prefHeight="30.0" text="Delete" HBox.hgrow="ALWAYS" />
                        <Button fx:id="sendBtn" mnemonicParsing="false" onAction="#handleCLick" prefHeight="30.0" text="Ajout Equipe" HBox.hgrow="ALWAYS" />

<AnchorPane fx:id="rootPane" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Esprit.Views.teamsScreen.ItemController">
   <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fitHeight="67.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Resources/clan.png" />
               </image></ImageView>
            <Label fx:id="name" prefHeight="61.0" text="eeeeeeeeeeeeeeeeeeeeeeeee" textFill="#ddcbcb" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold" size="31.0" />
               </font>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" />
   </padding>
</AnchorPane>
